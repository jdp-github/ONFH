<view class="cu-custom" style="height:{{CustomBar}}px;">
    <view class="cu-bar fixed bg-gradual-green" style="height:{{CustomBar}}px;padding-top:{{StatusBar}}px;">
        <navigator class='action border-custom' open-type="navigateBack" delta="1" hover-class="none" style='width:{{Custom.width}}px;height:{{Custom.height}}px;margin-left:calc(750rpx - {{Custom.right}}px)'>
            <text class='icon-back'></text>
            <text class='icon-homefill'></text>
        </navigator>
        <view class='content' style='top:{{StatusBar}}px;'>病历录入</view>
    </view>
</view>
<view class="cu-bar bg-white fixed" style="top:{{CustomBar}}px;">
    <view class="flex">
        <view class='avatar-group'>
            <view class="cu-avatar-group">
                <view class='cu-tag radius' style="margin-right: 40rpx;">填</view>
                <view class="cu-avatar round" style="background-image:url({{addAvatar}});"></view>
            </view>
        </view>
    </view>
    <view class="flex">
        <view class='avatar-group'>
            <view class="cu-avatar-group">
                <view wx:if="{{updateAvatarArr.length > 0}}">
                    <view class='cu-tag radius' style="margin-right: 40rpx;">修</view>
                    <view class="cu-avatar round" wx:for="{{updateAvatarArr}}" wx:key style="background-image:url({{item}});"></view>
                </view>
                <view wx:else>
                    <view class='cu-tag radius' style="margin-right: 10rpx;">修</view>
                    <view class='cu-tag radius'>暂无</view>
                </view>
            </view>
        </view>
    </view>
    <view class="flex">
        <view class='avatar-group'>
            <view class="cu-avatar-group">
                <view wx:if="{{approveAvatar.length > 0}}">
                    <view class='cu-tag radius' style="margin-right: 40rpx;">审</view>
                    <view class="cu-avatar round" style="background-image:url({{approveAvatar}});"></view>
                </view>
                <view wx:else>
                    <view class='cu-tag radius' style="margin-right: 10rpx;">审</view>
                    <view class='cu-tag radius'>暂无</view>
                </view>
            </view>
        </view>
    </view>
</view>
<scroll-view scroll-y class="indexes" scroll-into-view="indexes-{{listCurID}}" style='padding-top: 100rpx;height:100%' scroll-with-animation="true" enable-back-to-top="true">
    <view class="VerticalBox" style="height: 100%;">
        <scroll-view class="VerticalNav nav" scroll-y scroll-with-animation scroll-top="{{VerticalNavTop}}" style="height:100%">
            <view class="cu-item {{index==TabCur?'text-green cur':''}}" style="font-size: 22rpx" wx:for="{{TabTitle}}" wx:key bindtap='tabSelect' data-id="{{index}}">
                {{item}}
            </view>
        </scroll-view>
        <!-- 基本信息 -->
        <scroll-view class="VerticalMain" scroll-y style="height:100%" wx:if="{{ShowBasic}}">
            <view class="padding-left-special">
                <view class='cu-bar solid-bottom bg-white'>
                    <view class='action'>
                        <text class='icon-title text-green'></text>基本信息</view>
                </view>
                <view class="cu-list menu menu-avatar">
                    <view class="cu-item-special">
                        <view class="cu-form-group disabled">
                            <view class="title">所属中心</view>
                            <input placeholder="暂无所属中心" class='radius' name='input' value="{{centerName}}" disabled></input>
                        </view>
                        <view class="cu-form-group">
                            <view class='title'>姓名</view>
                            <input placeholder="请输入姓名" class='radius' bindinput="onNameInput" value="{{name}}"></input>
                        </view>
                        <view class="cu-form-group">
                            <view class='title'>ID</view>
                            <input placeholder="请输入病历号" class='radius' bindinput="onCaseNOInput" value="{{caseNO}}"></input>
                        </view>
                        <view class="cu-form-group">
                            <view class='title'>首诊日期</view>
                            <picker mode="date" value="{{createDate}}" start="2015-01-01" end="2025-01-01" bindchange="onCreateDateChange">
                                <view class="picker">
                                    {{createDate}}
                                </view>
                            </picker>
                        </view>
                        <view class="cu-form-group">
                            <view class='title'>性别</view>
                            <picker bindchange="onSexChange" value="{{sex}}" range="{{sexPicker}}">
                                <view class="picker">
                                    {{sexPicker[sex]}}
                                </view>
                            </picker>
                        </view>
                        <view class="cu-form-group">
                            <view class='title'>年龄</view>
                            <input placeholder="请输入年龄" class='radius' bindinput="onAgeInput" type="digit" value="{{age}}"></input>
                        </view>
                        <view class="cu-form-group">
                            <view class='title'>身高(m)</view>
                            <input placeholder="请注意单位换算" class='radius' bindinput="onHeightInput" type="digit" value="{{height}}"></input>
                        </view>
                        <view class="cu-form-group">
                            <view class='title'>体重(Kg)</view>
                            <input placeholder="请注意单位换算" class='radius' bindinput="onWeightInput" type="digit" value="{{weight}}"></input>
                        </view>
                        <view class="cu-form-group disabled">
                            <view class='title'>BMI(Kg/m2)</view>
                            <input class='radius' name='input' value="{{bmi}}" placeholder="自动生成" disabled></input>
                        </view>
                        <view class="cu-form-group">
                            <view class='title'>主诊医师</view>
                            <input placeholder="请输入主诊医师姓名" class='radius' bindinput="onChiefDocInput" value="{{chiefDoc}}"></input>
                        </view>
                        <view class="cu-form-group">
                            <view class='title'>联系电话1</view>
                            <input placeholder="请输入联系方式" class='radius' bindinput="onTel1Input" value="{{tel1}}"></input>
                        </view>
                        <view class="cu-form-group {{tel2Disabled?'disabled':''}}">
                            <view class='title'>联系电话2</view>
                            <input placeholder="请输入联系方式" class='radius' bindinput="onTel2Input" value='{{tel2}}' disabled='{{tel2Disabled}}'></input>
                            <switch class='green sm' bindchange="onTel2SwitchChange" checked='{{!tel2Disabled}}'></switch>
                        </view>
                        <view class="cu-form-group">
                            <view class='title'>简要病史</view>
                            <input placeholder='请输入文字, 没有请填"无"' class='radius' bindinput="onJybsInput" value="{{jybs}}"></input>
                        </view>
                        <view class="cu-form-group">
                            <view class='title'>其他伴随疾病</view>
                            <input placeholder='请输入文字, 没有请填"无"' class='radius' bindinput="onQtbsjbInput" value="{{qtbsjb}}"></input>
                        </view>
                    </view>
                </view>
            </view>
            <!-- 底部按钮 -->
            <view class="btn-block bg-white shadow" hidden='{{isLook}}'>
                <view class="flex margin-special">
                    <button class='cu-btn line-green shadow' style="margin-right: 15rpx;" form-type="submit" bindtap='submit' disabled='{{caseInfo.base_submit_button == 0}}'>
                        提交录入
                    </button>
                    <button class='cu-btn bg-green shadow' style="margin-left: 15rpx;" form-type="submit" bindtap='verify' disabled='{{isCreateCase || caseInfo.approve_button == 2}}'>
                        {{caseInfo.base.base_state != 2 ?"确认审核":"解除锁定"}}
                    </button>
                </view>
            </view>
        </scroll-view>
        <!-- 左侧 -->
        <scroll-view class="VerticalMain" scroll-y style="height:100%" wx:if="{{ShowDiagnose}}">
            <view class="padding-left-special">
                <view class='cu-bar solid-bottom bg-white'>
                    <view class='action'>
                        <text class='icon-title text-green'></text>左侧</view>
                </view>
                <!-- item -->
                <view class="cu-list menu menu-avatar">
                    <view class="cu-item-special">
                        <!-- 症状出现时长 -->
                        <view class="cu-form-group {{symptomDisabled?'disabled':''}}" bindtap="defaultSymptomDate">
                            <view class='title'>症状出现时长</view>
                            <picker mode="multiSelector" disabled='{{symptomDisabled}}' bindchange="symptomDateChange" bindcolumnchange="symptomDateColumnChange" value="{{symptomDateMultiIndex}}" range="{{symptomDateMultiArray}}">
                                <view class="picker">
                                    {{symptomDateValue?symptomDateValue:'请选择'}}
                                </view>
                            </picker>
                            <switch class='green sm ' bindchange="onSymptomSwitchChange" style='margin-left: 15rpx; ' checked='{{!symptomDisabled}}'></switch>
                        </view>
                        <!-- 首诊治疗情况 -->
                        <view class="cu-form-group {{sszlDisabled?'disabled':''}}">
                            <view class='title'>首诊治疗情况</view>
                            <picker bindchange="onSzzlChange" value="{{sszlIndex}}" range="{{sszlPicker}}" disabled='{{sszlDisabled}}'>
                                <view class="picker">
                                    {{sszlPicker[sszlIndex]}}
                                </view>
                            </picker>
                            <switch class=' green sm' bindchange="onSzzlSwitchChange" style='margin-left: 15rpx; ' checked='{{!sszlDisabled}}'></switch>
                        </view>
                        <!-- 类型 -->
                        <view class="cu-form-group {{leixingDisabled?'disabled ':' '}}">
                            <view class='title '>类型</view>
                            <picker bindchange="onLeixingChange" value="{{leixingIndex}}" range="{{leixingPicker}}" disabled='{{leixingDisabled}}'>
                                <view class="picker">
                                    {{leixingPicker[leixingIndex]}}
                                </view>
                            </picker>
                            <switch class='green sm' bindchange="onLeixingSwitchChange" style='margin-left: 15rpx; ' checked='{{!leixingDisabled}}'></switch>
                        </view>
                        <!-- 理由 -->
                        <view class="cu-form-group {{liyouDisabled?'disabled':''}}">
                            <view style='display:flex;flex-direction: column; '>
                                <view class='title' style='text-align:left'>理由</view>
                                <input placeholder="请输入文字" class='radius' style='width: 350rpx;text-align:left;' bindinput="onLiyouInput" value='{{liyou}}' disabled='{{liyouDisabled}}'></input>
                            </view>
                            <switch class=' green sm ' bindchange="onLiyouSwitchChange" style='margin-left: 15rpx; ' checked='{{!liyouDisabled}}'></switch>
                        </view>
                        <!-- ARCO分期 -->
                        <view class="cu-form-group">
                            <view class='title '>ARCO分期</view>
                            <picker bindchange="onArcoChange" value="{{arcoIndex}}" range="{{arcoPicker}}">
                                <view class="picker">
                                    {{arcoPicker[arcoIndex]}}
                                </view>
                            </picker>
                        </view>
                        <!-- Harris评分 -->
                        <view class="cu-form-group">
                            <view class='title '>Harris评分</view>
                            <input placeholder="请输入数字" class='radius ' bindinput="onHarrisInput" value="{{harris}}"></input>
                            <button class='cu-btn round sm ' style='margin-left: 15rpx;' bindtap="showModal" data-target="threeImgModal" data-imgtype="Harris">上传图片</button>
                        </view>
                        <!-- VAS疼痛评分 -->
                        <view class="cu-form-group">
                            <view class='title '>VAS疼痛评分</view>
                            <picker bindchange="onVASChange" value="{{vasIndex}}" range="{{vasPicker}}">
                                <view class="picker">
                                    {{vasPicker[vasIndex]}}
                                </view>
                            </picker>
                            <button class='cu-btn round sm ' style='margin-left: 15rpx; ' bindtap="showModal" data-target="threeImgModal" data-imgtype="VAS">上传图片</button>
                        </view>
                        <!-- 典型X光（左） -->
                        <view class="cu-form-group">
                            <view class='title '>典型X光（左）</view>
                            <picker bindchange="onXLeftChange" value="{{xleftIndex}}" range="{{xleftPicker}}">
                                <view class="picker">
                                    {{xleftPicker[xleftIndex]}}
                                </view>
                            </picker>
                            <button class='cu-btn round sm ' style='margin-left: 15rpx; ' bindtap="showModal" data-target="sixImgModal" data-imgtype="X-LEFT">上传图片</button>
                        </view>
                        <!-- 典型CT（左） -->
                        <view class="cu-form-group">
                            <view class='title '>典型CT（左）</view>
                            <picker bindchange="onCTLeftChange" value="{{ctLeftIndex}}" range="{{ctLeftPicker}}">
                                <view class="picker">
                                    {{ctLeftPicker[ctLeftIndex]}}
                                </view>
                            </picker>
                            <button class='cu-btn round sm ' style='margin-left: 15rpx; ' bindtap="showModal" data-target="sixImgModal" data-imgtype="CT-LEFT">上传图片</button>
                        </view>
                        <!-- 典型MRI（左） -->
                        <view class="cu-form-group">
                            <view class='title '>典型MRI（左）</view>
                            <picker bindchange="onMRILeftChange" value="{{mriLeftIndex}}" range="{{mriLeftPicker}}">
                                <view class="picker">
                                    {{mriLeftPicker[mriLeftIndex]}}
                                </view>
                            </picker>
                            <button class='cu-btn round sm ' style='margin-left: 15rpx; ' bindtap="showModal" data-target="sixImgModal" data-imgtype="MRI-LEFT">上传图片</button>
                        </view>
                        <!-- 本次治疗方案 -->
                        <view class="cu-form-group {{bczlDisabled?'disabled ':' '}}">
                            <view class='title '>本次治疗方案</view>
                            <picker bindchange="onBczlChange" value="{{bczlIndex}}" range="{{bczlPicker}}" disabled='{{bczlDisabled}}'>
                                <view class="picker">
                                    {{bczlPicker[bczlIndex]}}
                                </view>
                            </picker>
                            <switch class='green sm ' bindchange="onBczlSwitchChange" style='margin-left: 15rpx; ' checked='{{!bczlDisabled}}'></switch>
                        </view>
                        <!-- 具体治疗措施 -->
                        <view class="cu-form-group {{jtzlDisabled?'disabled ':' '}}">
                            <view class='title '>具体治疗措施</view>
                            <input placeholder="请输入文字" class='radius ' bindinput="onJtzlInput" value='{{jtzl}}' disabled='{{jtzlDisabled}}'></input>
                            <switch class='green sm ' bindchange="onJtzlSwitchChange" checked='{{!jtzlDisabled}}'></switch>
                        </view>
                    </view>
                </view>
            </view>
            <!-- 底部按钮 -->
            <view class="btn-block bg-white shadow" hidden='{{isLook}} '>
                <view class="flex margin-special">
                    <button class='cu-btn line-green shadow ' style="margin-right: 15rpx;" form-type="submit" bindtap='submit ' disabled='{{caseInfo.puncture.puncture_submit_button==0 }} '>
                        提交录入
                    </button>
                    <button class='cu-btn bg-green shadow ' style="margin-left: 15rpx;" form-type="submit" bindtap='verify ' disabled='{{isCreateCase || caseInfo.approve_button==2 }} '>
                        {{caseInfo.puncture.puncture_state != 2 ?"确认审核":"解除锁定"}}
                    </button>
                </view>
            </view>
        </scroll-view>
        <!-- 右侧 -->
        <scroll-view class="VerticalMain" scroll-y style="height:100%" wx:if="{{ShowAdmission}}">
            <view class="padding-left-special">
                <view class='cu-bar solid-bottom bg-white '>
                    <view class='action '>
                        <text class='icon-title text-green '></text>右侧</view>
                </view>
                <view class="cu-list menu menu-avatar">
                    <view class="cu-item-special">
                        <!-- 与假体相通的窦道 -->
                        <view class="cu-form-group">
                            <view class='title '>与假体相通的窦道</view>
                            <picker bindchange="onDoudaoChange" value="{{doudaoIndex}}" range="{{doudaoPicker}}">
                                <view class="picker">
                                    {{doudaoPicker[doudaoIndex]}}
                                </view>
                            </picker>
                        </view>
                        <!-- 术前ESR (mm/h) -->
                        <view class="cu-form-group">
                            <view class='title '>术前ESR(mm/h)</view>
                            <input placeholder="请输入数字" class='radius ' name='input ' type="number" value="{{sqesr}}" bindinput="onSqesrChange"></input>
                        </view>
                        <!-- 术前CRP (mg/dl)-->
                        <view class="cu-form-group">
                            <view class='title '>术前CRP(mg/dl)</view>
                            <input placeholder="请输入数字，注意单位换算" class='radius ' name='input ' type="digit" value="{{sqcrp}}" bindinput="onSqcrpChange"></input>
                        </view>
                        <!-- 标准化的CRP (mg/L) -->
                        <view class="cu-form-group disabled">
                            <view class='title '>标准化的CRP(mg/L)</view>
                            <input class='radius ' placeholder='默认生成，术前CRP乘10 ' name='input ' type="number" value="{{bzhcrp}}" disabled='true '></input>
                        </view>
                        <!-- IL-6 (pg/ml) -->
                        <view class="cu-form-group {{il6Disabled?'disabled ':' '}}">
                            <view style='display:flex;flex-direction: column; '>
                                <view class='title ' style='text-align:left '>IL-6(pg/ml)</view>
                                <input placeholder="请输入数字，未测请点右侧" class='radius ' disabled='{{il6Disabled}} ' type="digit" style='width: 350rpx;text-align:left; ' bindinput="onIl6Change" value='{{il6}} '></input>
                            </view>
                            <switch class='green sm ' bindchange="onIl6SwitchChange" style='margin-left: 15rpx; ' checked='{{!il6Disabled}} '></switch>
                        </view>
                        <!-- 纤维蛋白原 (μg/ml) -->
                        <view class="cu-form-group {{xwdbyDisabled?'disabled ':' '}}">
                            <view style='display:flex;flex-direction: column; '>
                                <view class='title ' style='text-align:left '>纤维蛋白原(μg/ml)</view>
                                <input placeholder="请输入数字，未测请点右侧" class='radius ' disabled='{{xwdbyDisabled}} ' type="digit" style='width: 350rpx;text-align:left; ' bindinput="onXwdbyChange" value='{{xwdby}} '></input>
                            </view>
                            <switch class='green sm ' bindchange="onXwdbySwitchChange" style='margin-left: 15rpx; ' checked='{{!xwdbyDisabled}} '></switch>
                        </view>
                        <!-- D-dimer (μg/ml) -->
                        <view class="cu-form-group {{ddimerDisabled?'disabled ':' '}}">
                            <view style='display:flex;flex-direction: column; '>
                                <view class='title ' style='text-align:left '>D-dimer(μg/ml)</view>
                                <input placeholder="请输入数字，未测请点右侧" class='radius ' disabled='{{ddimerDisabled}} ' type="digit" style='width: 350rpx;text-align:left; ' bindinput="onDdimerChange" value='{{ddimer}} '></input>
                            </view>
                            <switch class='green sm ' bindchange="onDdimerSwitchChange" style='margin-left: 15rpx; ' checked='{{!ddimerDisabled}} '></switch>
                        </view>
                        <!-- 手术日期 -->
                        <view class="cu-form-group  {{ssDateDisabled?'disabled ':' '}}">
                            <view class='title '>手术日期</view>
                            <picker mode="date" value="{{shoushuDate}}" start="2015-01-01" end="2025-01-01" bindchange="onShoushuDateChange" disabled='{{ssDateDisabled}} '>
                                <view class="picker">
                                    {{shoushuDate}}
                                </view>
                            </picker>
                            <switch class='green sm ' bindchange="onShoushuSwitchChange" style='margin-left: 15rpx; ' checked='{{!ssDateDisabled}} '></switch>
                        </view>
                        <!-- 术中见脓 -->
                        <view class="cu-form-group {{szjnDisabled?'disabled ':' '}}">
                            <view class='title '>术中见脓</view>
                            <picker bindchange="onSzjnChange" value="{{szjnIndex}}" range="{{szjnPicker}}" disabled='{{szjnDisabled}} '>
                                <view class="picker">
                                    {{szjnPicker[szjnIndex]}}
                                </view>
                            </picker>
                            <switch class='green sm ' bindchange="onSzjnSwitchChange" style='margin-left: 15rpx; ' checked='{{!szjnDisabled}} '></switch>
                        </view>
                        <!-- 病理 -->
                        <view class="cu-form-group {{blDisabled?'disabled ':' '}}">
                            <view class='title '>病理</view>
                            <picker bindchange="onBlChange" value="{{blIndex}}" range="{{blPicker}}" disabled='{{blDisabled}} '>
                                <view class="picker">
                                    {{blPicker[blIndex]}}
                                </view>
                            </picker>
                            <switch class='green sm ' bindchange="onBlSwitchChange" style='margin-left: 15rpx; ' checked='{{!blDisabled}} '></switch>
                        </view>
                        <!-- 术中LE试纸（原液） -->
                        <view class="cu-form-group {{szLEDisabled?'disabled ':' '}}">
                            <view class='title '>术中LE试纸(原液)</view>
                            <picker bindchange="onSZLEChange" value="{{szLEIndex}}" range="{{szLEPicker}}" disabled='{{szLEDisabled}} '>
                                <view class="picker">
                                    {{szLEPicker[szLEIndex]}}
                                </view>
                            </picker>
                            <button class='cu-btn round sm ' style='margin-left: 15rpx; ' disabled='{{szLEDisabled}} ' bindtap="showModal" data-target="LESecondDrawerModalR">上传图片</button>
                            <switch class='green sm ' bindchange="onSZLESwitchChange" style='margin-left: 15rpx; ' checked='{{!szLEDisabled}} '></switch>
                        </view>
                        <!--术中LE试纸（离心后） -->
                        <view class="cu-form-group {{szLEAfterDisabled?'disabled ':' '}}">
                            <view class='title '>术中LE试纸(离心后)</view>
                            <picker bindchange="onSZLEAfterChange" value="{{szLEAfterIndex}}" range="{{szLEAfterPicker}}" disabled='{{szLEAfterDisabled}} '>
                                <view class="picker">
                                    {{szLEAfterPicker[szLEAfterIndex]}}
                                </view>
                            </picker>
                            <button class='cu-btn round sm ' style='margin-left: 15rpx; ' disabled='{{szLEAfterDisabled}} ' bindtap="showModal" data-target="LESecondAfterDrawerModalR">上传图片</button>
                            <switch class='green sm ' bindchange="onSZLEAfterSwitchChange" style='margin-left: 15rpx; ' checked='{{!szLEAfterDisabled}} '></switch>
                        </view>
                        <!-- 术中关节液白细胞计数（cells/μL） -->
                        <view class="cu-form-group {{szgjybxbDisabled?'disabled ':' '}}">
                            <view style='display:flex;flex-direction: column; '>
                                <view class='title ' style='text-align:left '>术中关节液白细胞计数(cells/μL)</view>
                                <input placeholder="请输入数字，请注意单位换算" class='radius ' style='width: 350rpx;text-align:left; ' type='number ' bindinput="onSzgjybxbInput" disabled='{{szgjybxbDisabled}} ' value='{{szgjybxb}} '></input>
                            </view>
                            <switch class='green sm ' bindchange="onSzgjybxbSwitchChange" style='margin-left: 15rpx; ' checked='{{!szgjybxbDisabled}} '></switch>
                        </view>
                        <!-- 术中关节液中性粒细胞百分比（0.01） -->
                        <view class="cu-form-group {{szgjyzxlDisabled?'disabled ':' '}}">
                            <view style='display:flex;flex-direction: column; '>
                                <view class='title ' style='text-align:left '>术中关节液中性粒细胞百分比(0.01)</view>
                                <input placeholder="请输入数字，请注意格式，保留小数点2位" class='radius ' style='width: 350rpx;text-align:left; ' type='digit ' bindinput="onSzgjyzxlInput" disabled='{{szgjyzxlDisabled}} ' value='{{szgjyzxl}} '></input>
                            </view>
                            <switch class='green sm ' bindchange="onSzgjyzxlSwitchChange" style='margin-left: 15rpx; ' checked='{{!szgjyzxlDisabled}} '></switch>
                        </view>
                        <!-- 术中组织培养结果1 -->
                        <view class="cu-form-group {{szzz1Disabled?'disabled ':' '}}">
                            <view style='display:flex;flex-direction: column; '>
                                <view class='title ' style='text-align:left '>术中组织培养结果1</view>
                                <input placeholder="请输入培养结果，不适用请点击右侧" class='radius ' style='width: 350rpx;text-align:left; ' bindinput="onSzzz1Input" disabled='{{szzz1Disabled}} ' value='{{szzz1}} '></input>
                            </view>
                            <switch class='green sm ' bindchange="onSzzz1SwitchChange" style='margin-left: 15rpx; ' checked='{{!szzz1Disabled}} '></switch>
                        </view>
                        <!-- 术中组织培养结果2 -->
                        <view class="cu-form-group {{szzz2Disabled?'disabled ':' '}}">
                            <view style='display:flex;flex-direction: column; '>
                                <view class='title ' style='text-align:left '>术中组织培养结果2</view>
